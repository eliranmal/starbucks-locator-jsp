<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
	<display-name>starbucks-locator</display-name>
	
	<servlet>
		<servlet-name>RoutingService</servlet-name>
		<servlet-class>com.starbucks.locator.controller.RoutingService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LocatorService</servlet-name>
		<servlet-class>com.starbucks.locator.controller.LocatorService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RoutingService</servlet-name>
		<url-pattern>/Controller</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LocatorService</servlet-name>
		<url-pattern>/LocatorService</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>